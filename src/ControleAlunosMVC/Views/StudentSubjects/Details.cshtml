@model ControleAlunosMVC.Models.StudentSubject
@{
    ViewData["Title"] = "Detalhes Matriculas";
}

<h2>@ViewData["Title"]</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.DateStudentSubject):
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DateStudentSubject)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.StudentId):
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Student.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.SubjectId):
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Subject.Name)
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-info">Editar</a>
    <a class="btn btn-primary" asp-action="Index">Voltar</a>

</div>